
package com.mycompany.varrer;

/**vamos varrer o vetor, se o vasalor da posição
 * for menor do que 5, escreva  menor do que 5
 * caso contrario escreva maior do que 5
 * @author senai
 */
public class Varrer {

    public static void main(String[] args) {
        
        int[] numero = new int[]{5};
        for (int i = 0;i<numero.length; i++){
              
            if(numero[i]<=5){
              System.out.println("igual ou menor que 5");
                
            }else{
                System.out.println("caso contrario escreva maior que 5");
            }  
        }
    }
}
